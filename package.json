{
  "name": "hyperlocal-delivery",
  "version": "1.0.0",
  "description": "Hyperlocal Delivery Tracking System for Small Shops",
  "private": true,
  "scripts": {
    "dev": "turbo run dev",
    "dev:web": "turbo run dev --filter=@hyperlocal/web",
    "dev:delivery": "turbo run dev --filter=@hyperlocal/delivery-app",
    "dev:backend": "turbo run dev --filter=@hyperlocal/backend",
    "build": "turbo run build",
    "build:web": "turbo run build --filter=@hyperlocal/web",
    "build:delivery": "turbo run build --filter=@hyperlocal/delivery-app",
    "build:backend": "turbo run build --filter=@hyperlocal/backend",
    "clean": "turbo run clean",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "seed": "node seed-data.js",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "devDependencies": {
    "prettier": "^3.0.0",
    "turbo": "^1.13.4",
    "typescript": "^5.0.0"
  },
  "packageManager": "npm@9.6.7",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "main": "seed-data.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pratapnarayan/hyperlocal-delivery-V1.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/pratapnarayan/hyperlocal-delivery-V1/issues"
  },
  "homepage": "https://github.com/pratapnarayan/hyperlocal-delivery-V1#readme"
}
